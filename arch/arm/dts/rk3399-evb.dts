		default-brightness-level = <200>;
		pwms = <&pwm0 0 25000 0>;
		pinctrl-names = "default";
		pinctrl-0 = <&bl_en>;
	};

	vcc5v0_sys: vcc5v0-sys {
		compatible = "regulator-fixed";
		regulator-name = "vcc5v0_sys";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
	};

	vcc_lcd: vcc-lcd {
		compatible = "regulator-fixed";
		regulator-name = "vcc_lcd";
		gpio = <&gpio4 30 GPIO_ACTIVE_HIGH>;
		startup-delay-us = <20000>;
		enable-active-high;
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-boot-on;
		vin-supply = <&vcc5v0_sys>;
	};

	panel: panel {
		compatible = "simple-panel-dsi";
		backlight = <&backlight>;
		enable-gpios = <&gpio1 RK_PA1 GPIO_ACTIVE_HIGH>;
		prepare-delay-ms = <50>;
		enable-delay-ms = <50>;

		display-timings {
			native-mode = <&timing0>;

			timing0: timing0 {
				clock-frequency = <155000000>;
				hactive = <1200>;
				vactive = <1920>;
				hfront-porch = <60>;
				hsync-len = <24>;
				hback-porch = <80>;
				vfront-porch = <14>;
				vsync-len = <2>;
				vback-porch = <10>;
				hsync-active = <0>;
				vsync-active = <0>;
				de-active = <0>;
				pixelclk-active = <0>;
			};
		};

		ports {
			panel_in: endpoint {
				remote-endpoint = <&edp_out>;
			};
		};
	};

};

